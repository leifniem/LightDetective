{
	"name": "lightdetective",
	"version": "4.0.2",
	"description": "A client-side preset extractor for Adobe Lightroom adjustments",
	"main": "index.html",
	"repository": {
		"type": "git",
		"url": "https://github.com/leifniem/LightDetective.git"
	},
	"scripts": {
		"dev": "vite",
		"build": "vite build"
	},
	"keywords": [
		"lightroom",
		"exif",
		"preset",
		"xmp",
		"adobe"
	],
	"exifreader": {
		"include": {
			"jpeg": true,
			"webp": true,
			"heic": true,
			"png": true,
			"xmp": true
		}
	},
	"author": "Leif Niemczik",
	"license": "ISC",
	"devDependencies": {
		"autoprefixer": "^10.4.16",
		"postcss-input-range": "^5.0.0",
		"postcss-preset-env": "^7.8.3",
		"vite": "^2.9.17"
	},
	"dependencies": {
		"exifreader": "^4.20.0",
		"file-saver": "^2.0.5"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}
